@model MvcSka.Models.ViewModel.TableViewModel

@{
  Layout = null;
}

<p>
  Pure Ajax
</p>
<div class="row">
  <div class="col-md-12">
    <table class="table table-bordered table-hover">
      <tr>
        <th>#</th>
        <th>類別</th>
        <th>日期</th>
        <th>金額</th>
      </tr>
      @foreach (var row in Model.List)
      {
        <tr>
          <td>@row.Id</td>
          <td>@row.Type</td>
          <td>@row.Date</td>
          <td>@row.Amount</td>
        </tr>
      }
    </table>
  </div>
</div>

